<h1>new#music</h1>

<%= simple_form_for @music do |f| %>
  <div class="form">
    <%= f.input :author_id, collection: Author.all, :label_method => :name, :value_method => :id %>
    <%= f.input :title, placeholder: 'Track name' %>
    <div class="submit">
      <%= f.submit class: 'btn btn-primary' %>
      <%= link_to "Back to index", lists_path, class:"btn btn-info" %>
    </div>
  </div>
<% end %>
